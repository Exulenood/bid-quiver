{
  "expo": {
    "name": "bid-quiver",
    "slug": "bid-quiver",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/bid_quiver_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/bid_quiver_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bid_quiver_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/bid_quiver_favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
